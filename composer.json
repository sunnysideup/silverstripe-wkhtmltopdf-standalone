{
  "name": "sunnysideup/wkhtmltopdf-standalone",
  "description": "Brings together a bunch of binaries for a shared server where you do not have the ability to install it",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "wkpdftohtml",
    "pdf maker",
    "install",
    "shared hosting"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\WKHtmlToPdfStandAlone\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/sake-npm-build-wkhtmltopdf-standalone.sh",
    "bin/sake-npm-watch-wkhtmltopdf-standalone.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
